executable            = MakeHistosAtBatch.sh
universe              = vanilla
output                = BatchLog/Histo.$(SAMPLE).$(ClusterId).$(ProcId).out
error                 = BatchLog/Histo.$(SAMPLE).$(ClusterId).$(ProcId).err
log                   = BatchLog/Histo.$(ClusterId).log
stream_output         = True
stream_error          = True

environment = "JOBWORKDIR=$ENV(PWD)"

+JobFlavour  = "microcentury"
request_cpus = 4
arguments    = $(SAMPLE) 4
queue SAMPLE from (
MCUL18_DY_MG
MCUL17_DY_MG
MCUL16APV_DY_MG
)

